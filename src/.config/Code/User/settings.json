{
    "debug.toolBarLocation": "docked",
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.scrollBeyondLastLine": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.views.branches.files.layout": "list",
    "gitlens.views.commits.files.layout": "tree",
    "npm.enableRunFromFolder": true,
    "python.defaultInterpreterPath": "/bin/python3",
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": true,
    "terminal.external.linuxExec": "kitty",
    "terminal.integrated.defaultLocation": "editor",
    "vim.handleKeys": {
        "<C-b>": false,
        "<C-g>": false,
        "<C-i>": false,
        "<C-m>": false,
        "<C-w>": false,
        "<C-y>": false,
    },
    "vim.normalModeKeyBindings": [
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ],
        },
        {
            "before": [
                "<leader>",
                "a"
            ],
            "commands": [
                "editor.action.toggleWordWrap"
            ],
        },
        {
            "before": [
                "<leader>",
                "n",
            ],
            "commands": [
                ":set number"
            ],
        },
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                ":set relativenumber"
            ],
        }
    ],
    "vim.joinspaces": false,
    "vim.vimrc.enable": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.settings.editor": "json",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.wrappingIndent": "indent",
    "editor.lineNumbers": "relative",
    "remote.containers.defaultExtensions": [
        "eamodio.gitlens",
        "github.vscode-pull-request-github"
    ],
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.toggleSidebarVisibility"
    ],
    "githubPullRequests.fileListLayout": "flat",
    "editor.wordWrap": "off",
    "python.testing.pytestEnabled": true,
    "php-cs-fixer.executablePath": "/usr/local/bin/php-cs-fixer",
    "markdown.preview.scrollPreviewWithEditor": false,
    "markdown.preview.scrollEditorWithPreview": false,
    "window.title": "${rootName}${separator}code",
    "window.titleBarStyle": "custom",
    "workbench.settings.useSplitJSON": true,
    "workbench.preferredDarkColorTheme": "One Dark Pro",
    "workbench.colorTheme": "One Dark Pro",
    "window.autoDetectColorScheme": true,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.preferredLightColorTheme": "Atom One Light",
    "explorer.confirmDragAndDrop": false,
    "rust-client.engine": "rust-analyzer",
    "rust-analyzer.inlayHints.enable": false,
    "rust-analyzer.lens.enable": false,
    "editor.inlayHints.enabled": false,
    "workbench.editorAssociations": {
        "*.bin": "hexEditor.hexedit"
    },
    "extensions.ignoreRecommendations": true,
    "workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "githubPullRequests.pullBranch": "never",
    "python.linting.mypyEnabled": true,
    "ledger.binary": "/usr/bin/ledger",
    "window.restoreWindows": "none",
    "githubPullRequests.createOnPublishBranch": "never",
    "terminal.integrated.scrollback": 5000,
}
